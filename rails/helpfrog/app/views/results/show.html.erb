<section id="results-page">
  <h2>Resources Near <%= @address %></h2>

  <div id="map-canvas"></div>

  <h3>Nearest Places:</h3>
  <ol>
    <% @places.each do |place| %>
      <li>
        <%= link_to place.name, places_path(place) %>

        <p>Category: <% place.categories.each do |cat| %>
            <a href="/categories/<%= cat.id %>"><%= cat.name %></a>
          <% end %></p></li>
    <% end %>
  </ol>
</section>
